{
    "version": 2,
    "builds": [
      {
        "src": "server/index.mjs",
        "use": "@vercel/node"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/server/index.mjs"
      },
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      }
    ]
  }
  